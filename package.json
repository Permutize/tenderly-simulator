{
  "name": "tenderly-simulator",
  "version": "0.0.2",
  "description": "Tenderly simulation connector",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "prettier": "prettier --config .prettierrc --write .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Permutize/tenderly-simulator.git"
  },
  "keywords": [
    "tenderly",
    "web3",
    "transaction",
    "simulation"
  ],
  "author": "haha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Permutize/tenderly-simulator/issues"
  },
  "homepage": "https://github.com/Permutize/tenderly-simulator#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "axios": "^1.2.3",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "axios": ">=0.19.0",
    "typescript": ">=2.9.7"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/node": "^18.11.18",
    "ethers": "^6.1.0"
  }
}
